generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("postgreSQL_URL")
}

model MovieList {
  id          String   @id @default(uuid())
  title       String
  releaseDate String
  url         String?
  status      Status   @default(notReleased)
  createAt    DateTime @default(now())
  updataAt    DateTime @updatedAt

  @@unique([title, releaseDate])
}

enum Status {
  notReleased
  firstRound
  leaveFirstRound
  secondRound
  leavesecondRound
  Streaming
}
